[package]
name = "serde_json5"
version = "0.2.1"
authors = ["Gary Bressler <geb@google.com>"]
description = "A Serde (de)serializer for JSON5."
license = "Apache-2.0 AND ISC"
repository = "https://github.com/google/serde_json5"
readme = "README.md"
keywords = ["json5", "json", "serde", "serialization"]
edition = "2021"

[dependencies]
pest = "2.0"
pest_derive = "2.0"
serde = "1.0"

[dev-dependencies]
matches = "0.1.8"
serde_derive = "1.0"
serde_json = "1.0"
