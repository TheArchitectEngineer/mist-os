import("//build/drivers.gni")

fuchsia_driver_package("overnet-usb-package") {
  package_name = "overnet-usb"
  driver_components = [
    "overnet-usb",
    "overnet-usb-service",
  ]
}

group("drivers") {
  deps = [
    "overnet-usb",
    "overnet-usb-service",
  ]
}

group("tests") {
  testonly = true
  deps = [
    "overnet-usb:tests",
    "overnet-usb-service:tests",
  ]
}
