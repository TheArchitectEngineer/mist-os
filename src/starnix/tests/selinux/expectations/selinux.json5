{
    actions: [
        {
            type: "expect_pass",
            matchers: [
                "*",
            ],
        },
        {
            type: "expect_failure",
            matchers: [
                // keep-sorted start
                "atsecure",
                "bounds",
                "bpf",
                "capable_file",
                "capable_net",
                "cgroupfs_label",
                "checkreqprot",
                "extended_socket_class",
                "file",
                "filesystem",
                "fs_filesystem",
                "glblub",
                "inet_socket/tcp",
                "ioctl",
                "key_socket",
                "keys",
                "mac_admin",
                "mmap",
                "msg",
                "netlink_socket",
                "nlmsg",
                "nnp_nosuid*",
                "notify",
                "overlay",
                "perf_event",
                "rxdir",
                "sem",
                "shm",
                "sysctl",
                "tun_tap",
                "userfaultfd",
                "watchkey",

                // keep-sorted end
            ],
        },
        {
            type: "skip",
            matchers: [
                // TODO: https://fxbug.dev/398663317 - This sub-directory was mistakenly added to the test list.
                "inet_socket",

                // TODO: https://fxbug.dev/398663317 - Test hangs, possibly because IP_PASSSEC is not implemented yet.
                "inet_socket/udp",

                // TODO: https://fxbug.dev/398696695 - Test sometimes hangs.
                "unix_socket",

                // TODO: https://fxbug.dev/319480589 - Starnix does not support dynamic kernel module loading.
                "module_load",
            ],
        },
    ],
}
