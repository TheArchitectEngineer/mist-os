{
  "name": "rust-analyzer",
  "version": "1.0.0",
  "description": "A Gemini extension that uses rust-analyzer to provide code intelligence for Rust.",
  "mcpServers": {
    "rust-analyzer": {
      "command": "./prebuilt/third_party/mcp-language-server/linux-x64/mcp-language-server",
      "args": [
        "-workspace",
        "./",
        "-lsp",
        "./prebuilt/third_party/rust-analyzer/rust-analyzer"
      ],
      "timeout": 600000
    }
  },
  "contextFileName": "GEMINI.md"
}
