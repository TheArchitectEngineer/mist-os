import("//build/fidl/fidl.gni")
import("//build/fidl/toolchain.gni")

group("test") {
  deps = [ ":fidl.power.observability($fidl_toolchain)" ]
}

fidl("fuchsia.power.observability") {
  sdk_category = "partner"
  sources = [ "constants.fidl" ]
  contains_drivers = true
  sdk_area = "Power"
  stable = false
}
